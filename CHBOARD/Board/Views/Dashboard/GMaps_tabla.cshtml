@*@model Board.Models.VM_Payload*@
@model IEnumerable<Board.Models.VM_Payload>
@{
    ViewBag.Title = "Home Page";
}

<div class="table-responsive">
    <table class="table table-bordered table-condensed table-striped table-hover sortable">
        @if (Model.Count() == 0)
        {
            <tr>
                <td colspan="10">No Record's found.</td>
            </tr>
        }
        else
        {
            <thead>
                <tr class="danger">
                    <th data-defaultsign="_19"> PayloadId</th>
                    <th data-defaultsign="_19"> DeviceId</th>
                    <th data-defaultsign="_19"> DatetimeInicio</th>
                    <th data-defaultsign="_19"> DatetimeFin</th>
                    <th data-defaultsign="_19"> Alt</th>
                    <th data-defaultsign="_19"> Hdop</th>
                    <th data-defaultsign="_19"> Info</th>
                    <th data-defaultsign="_19"> Latitud</th>
                    <th data-defaultsign="_19"> Longitud</th>
                    <th data-defaultsign="_19"> DeviceName</th>
                    <th data-defaultsign="_19"> DevEui</th>
                    <th data-defaultsign="_19"> DevAddr</th>
                    <th data-defaultsign="_19"> ApplicationId</th>
                    <th data-defaultsign="_19"> ApplicationName</th>
                    <th data-defaultsign="_19"> Gateway</th>
                </tr>
            </thead>

            foreach (var item in Model)
            {
                <tr>
                    <td>@item.PayloadId</td>
                    <td>@item.DeviceId</td>
                    <td>@item.DatetimeInicio</td>
                    <td>@item.DatetimeFin</td>
                    <td>@item.Alt</td>
                    <td>@item.Hdop</td>
                    <td>@item.Info</td>
                    <td>@item.Latitud</td>
                    <td>@item.Longitud</td>
                    <td>@item.DeviceName</td>
                    <td>@item.DevEui</td>
                    <td>@item.DevAddr</td>
                    <td>@item.ApplicationId</td>
                    <td>@item.ApplicationName</td>
                    <td>@item.Gateway</td>
                </tr>

            }
        }
    </table>
</div>